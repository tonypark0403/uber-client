{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo": "^2.27.3",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "axios": "^0.19.2",
    "global": "^4.4.0",
    "google-maps-react": "^2.0.6",
    "graphql": "^14.3.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-helmet": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-sidebar": "^3.0.2",
    "react-toastify": "^5.5.0",
    "styled-components": "^5.1.0",
    "styled-reset": "^4.1.4",
    "subscriptions-transport-ws": "^0.9.16",
    "tslint-react": "^5.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/googlemaps": "^3.39.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-facebook-login": "^4.1.1",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-sidebar": "^3.0.0",
    "@types/react-toastify": "^4.1.0",
    "@types/styled-components": "^5.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precodegen": "apollo schema:download --endpoint=https://uber-srv.herokuapp.com/graphql",
    "codegen": "apollo codegen:generate src/types/api.d.ts --queries=src/**/*Queries.ts --addTypename --localSchemaFile=schema.json --target typescript --outputFlat",
    "lint": "tslint -p tsconfig.json -c ./tslint.json",
    "format": "tslint-config-prettier-check ./tslint.json",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn lint",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://tonypark0403.github.io/uber-client/"
}
